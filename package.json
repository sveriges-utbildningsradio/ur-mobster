    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.1.2",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "opencollective-postinstall": "^2.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "spectron": "^5.0.0",
